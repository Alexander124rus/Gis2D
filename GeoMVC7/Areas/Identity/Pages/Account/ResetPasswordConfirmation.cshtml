@page
@model ResetPasswordConfirmationModel
@{
    ViewData["Title"] = "Сброс подтверждения пароля";
}

<div class="container marketing">
    <main class="form-signin w-50 m-auto">
        <h1>@ViewData["Title"]</h1>
        <p>
            Ваш пароль был сброшен. Пожалуйста <a asp-page="./Login">нажмите здесь, чтобы войти в систему</a>.
        </p>
    </main>
</div>
